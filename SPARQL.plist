        <string>WHERE</string>
        <string>CONSTRUCT</string>
        <string>DESCRIBE</string>
        <string>ASK</string>
        <string>FROM</string>
        <string>GRAPH</string>
        <string>PREFIX</string>
        <string>BASE</string>
        <string>FILTER</string>
        <string>OPTIONAL</string>
        <string>UNION</string>
        <string>MINUS</string>
        <string>BIND</string>
        <string>SERVICE</string>
        <string>VALUES</string>
        <string>ORDER</string>
        <string>BY</string>
        <string>LIMIT</string>
        <string>OFFSET</string>
        <string>GROUP</string>
        <string>HAVING</string>
        <string>DISTINCT</string>
        <string>REDUCED</string>
        <string>AS</string>
        <string>NOT</string>
        <string>IN</string>
        <string>EXISTS</string>
        <string>LOAD</string>
        <string>CLEAR</string>
        <string>DROP</string>
        <string>ADD</string>
        <string>MOVE</string>
        <string>COPY</string>
        <string>CREATE</string>
        <string>INSERT</string>
        <string>DELETE</string>
        <string>WITH</string>
        <string>USING</string>
    </array>

    <key>BBLMSuffixMap</key>
    <array>
        <dict>
            <key>BBLMLanguageSuffix</key>
            <string>.rq</string>
        </dict>
        <dict>
            <key>BBLMLanguageSuffix</key>
            <string>.sparql</string>
        </dict>
    </array>

    <key>BBLMPreferredFilenameExtension</key>
    <string>rq</string>

    <key>Language Features</key>
    <dict>
        <key>Identifier and Keyword Character Class</key>
        <string>A-Za-z0-9_-</string>
        <key>Prefix and Suffix Word Characters</key>
        <string>:</string>
        
        <key>Open Line Comments</key>
        <string>#</string>

        <key>Open Strings 1</key>
        <string>"</string>
        <key>Close Strings 1</key>
        <string>"</string>
        <key>Escape Char in Strings 1</key>
        <string>\</string>
